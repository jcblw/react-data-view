    "babel-core": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.0",
    "eslint-plugin-react": "^3.16.1"
  },
  "babel": {
